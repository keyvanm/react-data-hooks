{
  "name": "react-use-rest",
  "version": "1.5.3",
  "description": "React.js data hooks for REST endpoints",
  "main": "index.js",
  "target": "esnext",
  "scripts": {
    "prebuild": "rm -rf build",
    "test": "jest --verbose",
    "dev": "yarn test - --watch",
    "build": "babel src --ignore '**/*.spec.js' --out-dir build -d && yarn test",
    "release": "yarn build && release --src=build --push --tag",
    "release:major": "yarn release --major",
    "release:minor": "yarn release --minor",
    "release:patch": "yarn release --patch",
    "release:next": "yarn build && release --src=build --push --type=next"
  },
  "keywords": [
    "react",
    "hooks",
    "api",
    "rest",
    "state"
  ],
  "author": "Kevin R. Whitley <krwhitley@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kwhitley/react-use-rest.git"
  },
  "bugs": {
    "url": "https://github.com/kwhitley/react-use-rest/issues",
    "email": "krwhitley@gmail.com"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/react-hooks": "^2.0.1",
    "chalk": "^2.4.2",
    "faker": "^4.1.0",
    "fetch-mock": "^7.3.9",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^2.1.2",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-test-renderer": "^16.9.0",
    "supergeneric": "^1.1.0",
    "whatwg-fetch": "^3.0.0",
    "yarn-release": "^1.10.2"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "deepmerge": "^4.0.0",
    "use-store-hook": "^1.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
